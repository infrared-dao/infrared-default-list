{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "protocols": {
      "items": {
        "properties": {
          "description": { "type": "string" },
          "id": { "type": "string" },
          "image": { "type": "string" },
          "name": { "type": "string" },
          "url": { "format": "uri", "type": "string" }
