{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "gauges": {
      "items": {
        "properties": {
          "beraRewardsVault": {
            "pattern": "^0x[a-fA-F0-9]{40}$",
            "type": "string"
          },
          "lpTokenAddress": {
            "pattern": "^0x[a-fA-F0-9]{40}$",
            "type": "string"
          },
          "mintUrl": { "format": "uri", "type": "string" },
          "name": { "type": "string" },
          "protocol": { "type": "string" },
          "types": {
            "items": { "type": "string" },
            "type": "array"
          },
          "underlyingTokens": {
            "items": { "pattern": "^0x[a-fA-F0-9]{40}$", "type": "string" },
            "type": "array"
          }
        },
        "required": [
          "beraRewardsVault",
          "lpTokenAddress",
          "mintUrl",
          "name",
          "protocol",
          "types",
          "underlyingTokens"
        ],
        "type": "object"
      },
      "type": "array"
    }
  },
  "required": ["gauges"],
  "type": "object"
}
