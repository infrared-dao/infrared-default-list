{
  "name": "@infrared-dao/default-lists",
  "version": "1.0.0",
  "description": "Default lists for Infrared interfaces including tokens, gauges, and validators",
  "keywords": [
    "berachain",
    "default",
    "token",
    "gauge",
    "validator",
    "list"
  ],
  "homepage": "https://github.com/infrared-dao/infrared-default-list#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infrared-dao/infrared-default-list.git"
  },
  "license": "GPL-3.0-or-later",
  "scripts": {
    "format": "prettier --cache --log-level warn --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "lint": "concurrently \"pnpm format\" \"pnpm type-check\"",
    "sort-package-json": "pnpm dlx sort-package-json",
    "type-check": "tsc --noEmit",
    "validate": "node scripts/validate-gauge-list.ts"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.7.6",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "concurrently": "^9.0.1",
    "prettier": "^3.3.1",
    "sharp": "^0.33.5",
    "typescript": "^5.6.3",
    "viem": "^2.21.28"
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=20.5.1",
    "npm": "please-use-pnpm",
    "pnpm": ">=8.6.1",
    "yarn": "please-use-pnpm"
  }
}
